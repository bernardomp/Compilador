PROGRAMA p1;
VAR a: ENTERO;
b:REAL;
c:BOOLEANO;
d: VECTOR [10] DE REAL;
INICIO
   MIENTRAS NO(b<=-45) O CIERTO HACER
    INICIO 
      b:=-1*9;
      ESCRIBE (c);
    FIN;
  SI NO (FALSO) Y (b>1.0) ENTONCES 
    INICIO
      d[3]:=0.6;
    FIN
  SINO 
    INICIO
      LEE(b);
    FIN;
FIN.
